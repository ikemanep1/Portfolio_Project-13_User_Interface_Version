<h1>Dogs!</h1>


<div class="dog-list">
  <% @dogs.each do |dog| %>
  <div class="dog-individual">
    <ul>
      <li><%= dog.fetch("name") %></li>
      <li><%= dog.fetch("breed") %></li>
      <li><%= dog.fetch("age") %></li>
    </ul>
  </div>

  <% end %>

  <div class="cat-list">
    <% @cats.each do |cat| %>
    <div class="cat-individual">
      <ul>
        <li><%= cat.fetch("name") %></li>
        <li><%= cat.fetch("breed") %></li>
        <li><%= cat.fetch("age") %></li>
      </ul>
    </div>

    <% end %>
