<h1>Dogs!</h1>
<div class="links">
  <a class="cat-link" href="cats/">Want to look at some cats?</a>
</div>


<div class="dog-list">
  <% @dogs.each do |dog| %>
  <div class="dog-individual">
    <ul>
      <li><%= dog.fetch("name") %></li>
      <li><%= dog.fetch("breed") %></li>
      <li><%= dog.fetch("age") %></li>
    </ul>
  </div>

  <% end %>
